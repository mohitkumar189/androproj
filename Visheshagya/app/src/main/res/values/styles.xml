<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NoActionBarTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">@color/black</item>
        <!--<item name="buttonStyle">@style/AppTheme.Button</item>-->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="ToolBarStyle.Base" parent="">
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <color name="actionBarText">#E4000E</color>
    <color name="Red">#E4000E</color>
    <color name="White">#FFFFFF</color>
    <color name="status_bar_color">#000000</color>
    <color name="colorPrimary">#fefefe</color>
    <color name="colorPrimaryDark">#E4000F</color>
    <color name="colorAccent">#E4000F</color>
    <color name="textColor">#000000</color>
    <color name="backgroundColor">#eaeaea</color>
    <color name="headingColor">#000000</color>
    <color name="black">#000000</color>
    <color name="websiteRed">#E4000F</color>
    <color name="headingBackground">#767676</color>
    <color name="TITLElABLE">#696969</color>


</resources>
